  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "./cmd/grpcServer/main.go",
      "console": "internalConsole",
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Lauch Node Client",
      "cwd": "${workspaceFolder}/cmd/grpcClientNode/",
      "program": "${workspaceFolder}/cmd/grpcClientNode/main.js",
      "env": {},
      "console": "internalConsole",
      "outputCapture": "std"
    },
  ]
}